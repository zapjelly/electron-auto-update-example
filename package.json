{
  "name": "electron-auto-update-example",
  "version": "0.0.4",
  "description": "",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zapjelly/electron-auto-update-example.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder build --mac --win --publish never",
    "deploy": "electron-builder electron-builder  -mw --publish always",
    "electron:package": "electron-builder -c.extraMetadata.main=main.js -mw --publish never"
  },
  "build": {
    "npmRebuild": false,
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "assets/mac/entitlements.mac.plist",
      "entitlementsInherit": "assets/mac/entitlements.mac.plist"
    },
    "dmg": {
      "sign": false
    }
  },
  "publish": {
    "provider": "github",
    "repo": "electron-auto-update-example",
    "owner": "zapjelly",
    "releaseType": "prerelease",
    "publishAutoUpdate": true
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "electron-is-dev": "^2.0.0",
    "path": "^0.12.7"
  },
  "dependencies": {
    "electron-auto-update": "^1.2.2",
    "electron-is-dev": "^2.0.0",
    "electron-osx-sign": "^0.5.0",
    "electron-updater": "^4.0.6"
  }
}